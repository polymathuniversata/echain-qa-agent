{
  "name": "echain-qa-agent",
  "version": "2.0.1",
  "description": "Comprehensive QA automation for blockchain and web3 projects - ensures code quality, runs tests, and updates documentation",
  "main": "dist/index.js",
  "bin": {
    "echain-qa": "./dist/cli.js"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "echo \"No tests specified yet\"",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "qa": "echain-qa run",
    "qa:lint": "echain-qa lint",
    "qa:test": "echain-qa test",
    "qa:security": "echain-qa security",
    "qa:build": "echain-qa build",
    "qa:init": "echain-qa init",
    "qa:ci": "./scripts/ci-pipeline.sh",
    "dev:qa": "./scripts/dev-with-qa.sh",
    "setup:hooks": "./scripts/setup-hooks.sh",
    "precommit": "echain-qa run --dry-run",
    "prepush": "echain-qa run"
  },
  "keywords": [
    "qa",
    "testing",
    "blockchain",
    "web3",
    "solidity",
    "typescript",
    "eslint",
    "security",
    "automation"
  ],
  "author": "Polymath Universata",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/polymathuniversata/echain-qa-agent.git"
  },
  "bugs": {
    "url": "https://github.com/polymathuniversata/echain-qa-agent/issues"
  },
  "homepage": "https://github.com/polymathuniversata/echain-qa-agent#readme",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.0",
    "bun-types": "^1.0.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}