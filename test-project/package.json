{
  "name": "test-qa-project",
  "version": "1.0.0",
  "description": "Test project for QA agent validation",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx echain-qa run --dry-run --quiet && cd frontend && tsc",
    "test": "npx echain-qa run --dry-run --quiet && cd frontend && jest",
    "lint": "cd frontend && eslint src/**/*.ts",
    "type-check": "cd frontend && tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.0",
    "glob": "^11.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  }
}